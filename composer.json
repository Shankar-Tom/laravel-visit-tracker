{
    "name": "shankar/laravel-visit-tracker",
    "description": "Track page and model visits (with morph visitor) + analytics.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shankar\\VisitTracker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shankar\\VisitTracker\\VisitTrackerServiceProvider"
            ],
            "aliases": {
                "VisitTracker": "Shankar\\VisitTracker\\Facades\\VisitTracker"
            }
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "minimum-stability": "dev"
}